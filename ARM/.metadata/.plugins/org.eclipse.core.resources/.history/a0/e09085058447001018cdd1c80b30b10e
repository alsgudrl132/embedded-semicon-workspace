#ifndef INC_LED_H_
#define INC_LED_H_

#include "main.h"

typedef struct {
	GPIO_TypeDef	*port;
	uint16_t		pinNumber;
	GPIO_PinState	onState;
	GPIO_PinState	offState;
}LED_STRUCT;


void ledOn();
void ledOff();

#endif /* INC_LED_H_ */
